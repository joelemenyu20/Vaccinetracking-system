    <servlet>
        <servlet-name>UserAuthentication</servlet-name>
        <servlet-class>org.user.login.UserAuthentication</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Patientdata</servlet-name>
        <servlet-class>org.health.administration.Patientdata</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Emaildata</servlet-name>
        <servlet-class>org.health.administration.Emaildata</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Certificatedata</servlet-name>
        <servlet-class>org.health.administration.Certificatedata</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Registration</servlet-name>
        <url-pattern>/Registration</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>User</servlet-name>
        <url-pattern>/User</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>loginValidation</servlet-name>
        <url-pattern>/loginValidation</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>UserRegistration</servlet-name>
        <jsp-file>/Myjsps/UserRegistration.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>UserRegistration</servlet-name>
        <url-pattern>/Myjsps/UserRegistration</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>PatientRegistration</servlet-name>
        <jsp-file>/Myjsps/PatientRegistration.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>PatientRegistration</servlet-name>
        <url-pattern>/Myjsps/PatientRegistration</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>booking</servlet-name>
        <url-pattern>/booking</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>HealthCenterAdmin</servlet-name>
        <url-pattern>/HealthCenterAdmin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>VaccineInventory</servlet-name>
        <url-pattern>/VaccineInventory</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserAuthentication</servlet-name>
        <url-pattern>/UserAuthentication</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>AdminDashboard</servlet-name>
