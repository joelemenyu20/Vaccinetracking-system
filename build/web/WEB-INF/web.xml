    </servlet>
    <servlet-mapping>
        <servlet-name>AdminDashboard</servlet-name>
        <url-pattern>/AdminDashboard</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>PatientDashboard</servlet-name>
        <jsp-file>/PatientDashboard.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>PatientDashboard</servlet-name>
        <url-pattern>/PatientDashboard</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Patientdata</servlet-name>
        <url-pattern>/Patientdata</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Emaildata</servlet-name>
        <url-pattern>/Emaildata</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Certificatedata</servlet-name>
        <url-pattern>/Certificatedata</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
